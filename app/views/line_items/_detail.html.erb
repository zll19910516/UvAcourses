<table><tr>	<td align="center">Course Name</td>	<td align="center">SGID</td>	<td align="center">Institute</td>	<td align="center">ETCS</td>	<td align="center">More</td>	<td align="center">Delete</td></tr><% @cart.line_items.each do |line_item| %>		<tr>	<td width="33%"><%= line_item.course.name %></td>	<td width="10%" align="center"><%= line_item.course.sgid%></td>	<td width="32%" align="center"><%=h line_item.course.institute%></td>	<td width="5%" align="center"><%= line_item.course.ects%></td>	<td width="10%" align="center"><%= link_to "[more...]", controller: "courses_store", action: "show", course_sgid: line_item.course.sgid, cart_id: @cart.id %></td>	<td width="10%" align="center"><%= button_to 'Delete', line_item, method: :delete, data: { confirm: 'Are you sure?' }, remote: true%></td>	</tr>	<% end %></table>